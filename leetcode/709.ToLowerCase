class Solution {
    public String toLowerCase(String str) {
        StringBuilder strb = new StringBuilder();
        for(int i=0;i<str.length();i++){
            char a=str.charAt(i);
            if(a<='Z'&&a>='A'){
                a=(char)((int)a-('A'-'a'));
                
            }
            strb.append(a);
        }
        return strb.toString();
    }
}
